<h1>DOCTORS</h1>
<table class="table">
  <tr>
    <th>First_name</th>
    <th>Last_name</th>
    <th>Email</th>
    <th>Shift_type</th>
    <th>Rate</th>
  </tr>
  {{#each doctors}}
  <tr>
    <td>
      <a href="/doctor/{{id}}">{{first_name}}</a>
      </td>
    <td>{{last_name}}</td>
    <td>{{email}}</td>
    <td>{{slot_type}}</td>
    <td>{{rate}}</td>
  </tr>
  {{/each}}
</table>
