<div class="bg-img">

  <h1> appointments</h1>
  <table class="table">
     <tr>
      <th>ID Number</th>
      <th>First_Name</th>
      <th>Last_Name</th>
      <th>Contact</th>
      <th>Reason</th>
      <th>Allergy</th>
      <th>First_Time_Visit</th>

    </tr>
    {{#each appointments}}
   
    <tr>
      <td>{{id_number}}</td>
      <td>{{first_name}}</td>
      <td>{{last_name}}</td>
      <td>{{contact_no}}</td>
      <td>{{reason}}</td>
      <td>{{allergy}}</td>
      <td>{{first_time_visit}}</td>

    </tr>
    {{/each}}
  </table>
</div>